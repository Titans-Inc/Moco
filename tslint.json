{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "adjacent-overload-signatures": true,
    "ban-ts-ignore": true,
    "interface-name": [true, "never-prefix"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-trailing-underscore", "require-const-for-all-caps"]
  },
  "rulesDirectory": []
}
